<div class="card mb-3" id="list-<%= list.client_id %>">
  <div class="card-header">
    <h6> New Task</h6>
  </div>
  <div class="card-body">
    <%= bootstrap_form_with model: list do |f| %>
      <%= f.hidden_field :client_id %>
      <%= f.text_field :name %>
      <%= f.primary %>
    <% end %>
  </div>
</div>